{
  "name": "kuririn-react-router",
  "private": false,
  "version": "3.0.0",
  "type": "module",
  "main": "./dist/lib/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jiqishoubi/kuririn-react-router"
  },
  "description": "a pages stack library for mobile H5 that can simulate the effect of page stacks in apps (or mini programs), achieve page push, back, and replace, and support browser forward and backward buttons",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier-all": "prettier --write '**/*.{js,jsx,ts,tsx,vue,css,less,scss,md,json}'",
    "pub": "npm run build && npm version patch && npm publish"
  },
  "dependencies": {
    "history": "^5.0.0"
  },
  "devDependencies": {
    "@types/history": "^5.0.0",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "classnames": "^2.3.2",
    "husky": "^8.0.3",
    "kuririn-react-router": "^3.0.0",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "react": "^18.0.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue,css,less,scss,md,json}": "prettier --write"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mobile",
    "pages",
    "stack",
    "router",
    "react",
    "H5"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
